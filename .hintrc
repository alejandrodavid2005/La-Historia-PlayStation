{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/parsing": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "box-align"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 123",
    "not and_ff <= 124",
    "not chrome <= 123",
    "not edge <= 123",
    "not firefox <= 124",
    "not ios_saf <= 17.4",
    "not opera <= 108",
    "not safari <= 17.4",
    "not samsung <= 23"
  ]
}